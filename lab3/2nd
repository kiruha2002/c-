#include <iostream>


using namespace std;


void Sort(int size, int* arr){
    int temp;
    for (int i = 0; i < size - 1; i++)
    {
        for (int j = 0; j < size - i - 1; j++)
        {
            if (arr[j] > arr[j + 1])
            {
                temp = arr[j];
                arr[j] = arr[j + 1];
                arr[j + 1] = temp;

            }
        }
    }
}

void ArrayInsert(int* arr,int size) {
    for (int i = 0; i < size; i++)
    {
        cout << "arr[" << i << "] = ";
        cin >> arr[i];
    }
}



int main()
{
    int* arr;
    int size;


    cout << " quantity = ";
    cin >> size;

    arr = new int[size];

    ArrayInsert(arr,size);

    Sort(size, arr);


    for (int i = 0; i < size; i++)
    {
        cout << arr[i] << " ";

    }
    cout << endl;



}
